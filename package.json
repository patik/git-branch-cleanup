{
    "name": "git-branch-cleanup",
    "version": "1.0.0",
    "engines": {
        "node": ">=18"
    },
    "description": "Remove local git branches which are no longer available on the remote",
    "main": "dist/index.js",
    "type": "module",
    "preferGlobal": true,
    "bin": {
        "git-branch-cleanup": "dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "tsx ./src/index.ts",
        "test": "pnpm run build && tsx ./src/test.ts",
        "lint": "tsc --noEmit && pnpm run format && eslint .",
        "format": "prettier --write --list-different .",
        "check-format": "prettier --check --list-different ."
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/patik/git-branch-cleanup.git"
    },
    "author": "Craig Patik",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/patik/git-branch-cleanup/issues"
    },
    "homepage": "https://github.com/patik/git-branch-cleanup",
    "dependencies": {
        "minimist": "^1.2.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.27.0",
        "@inquirer/prompts": "^7.5.1",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.15.18",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "prettier": "^3.5.3",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.1"
    }
}
